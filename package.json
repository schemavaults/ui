{
  "name": "@schemavaults/ui",
  "version": "0.11.23",
  "private": false,
  "license": "UNLICENSED",
  "description": "React.js UI components for SchemaVaults frontend applications",
  "repository": {
    "type": "git",
    "url": "https://github.com/schemavaults/ui.git"
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-context-menu": "^2.2.2",
    "@radix-ui/react-dialog": "1.1.14",
    "@radix-ui/react-dropdown-menu": "2.1.2",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "1.2.1",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-table": "8.20.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "^1.0.0",
    "eslint": "9.17.0",
    "framer-motion": "12.16.0",
    "lucide-react": "0.508.0",
    "next-themes": "0.4.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.55.0",
    "tailwind-merge": "3.0.2",
    "zod": "3.23.8",
    "@schemavaults/theme": "0.21.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-onboarding": "8.6.14",
    "@storybook/addon-webpack5-compiler-swc": "3.0.0",
    "@storybook/blocks": "8.6.14",
    "@storybook/react-webpack5": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/test": "8.6.14",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "bun-types": "1.2.11",
    "eslint-plugin-storybook": "0.12.0",
    "storybook": "8.6.14",
    "tsc-alias": "1.8.16",
    "typescript": "5.8.3",
    "ignore-loader": "0.1.2",
    "postcss": "8.5.3",
    "postcss-loader": "8.1.1",
    "tailwindcss": "3.4.17",
    "autoprefixer": "10.4.21"
  },
  "scripts": {
    "build:pkg": "tsc --project tsconfig.pkg.json && tsc-alias --project tsconfig.pkg.json",
    "build:storybook": "storybook build",
    "build": "bun run build:pkg",
    "prebuild": "bun run clean",
    "clean": "rm -rf ./dist",
    "storybook": "storybook dev -p 6006",
    "preview": "bun run storybook"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*",
      "import": "./dist/*",
      "require": "./dist/*"
    },
    "./dist": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./dist/*": {
      "types": "./dist/*",
      "import": "./dist/*",
      "require": "./dist/*"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.js",
      "require": "./dist/providers/index.js"
    },
    "./providers/*": {
      "types": "./dist/providers/*",
      "import": "./dist/providers/*",
      "require": "./dist/providers/*"
    },
    "./framer-motion": {
      "types": "./dist/framer-motion.d.ts",
      "import": "./dist/framer-motion.js",
      "require": "./dist/framer-motion.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "bun@1.2.11",
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
